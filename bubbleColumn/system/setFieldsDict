FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.air 0.0
    
    volScalarFieldValue T.air 298.59
    volScalarFieldValue T.water 298.59
    volScalarFieldValue p 6000000
);

regions
(
    boxToCell
    {
        box (-250 -1 -1) (6 1 1);
        fieldValues
        (
          volScalarFieldValue alpha.air 0.0 volScalarFieldValue T.air
          298.59 volScalarFieldValue T.water 298.59 volScalarFieldValue
              p 6000000
        );
    }
);


