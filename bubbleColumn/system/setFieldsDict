FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.air 1.0
);

regions
(
    boxToCell
    {
        box (-50 -1 -1) (6 1 1);
        fieldValues
        (
            volScalarFieldValue T.air 280 volScalarFieldValue T.water 280 volScalarFieldValue p 2000000
        );
    }
);


