FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.air 0.2
    
    volScalarFieldValue T.air 295.13
    volScalarFieldValue T.water 295.13
    volScalarFieldValue p 6000000
);

regions
(
    boxToCell
    {
        box (-17.029 -1 -1) (6 1 1);
        fieldValues
        (
            volScalarFieldValue alpha.air 0.99 volScalarFieldValue T.air 240 volScalarFieldValue T.water 240 volScalarFieldValue p 1282500
        );
    }
);


