FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    object      U.water;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    sideLeft
    {
        type               symmetryPlane;
    }
    sideRight
    {
        type               zeroGradient;
	//value              uniform (0 0 0); 
    }
    topbottom
    {
    	type               empty;
    }
    frontback
    {
    	type               empty;
    }
}

